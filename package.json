{
  "name": "mlabs-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "init-husky": "husky install && chmod +x .husky/pre-commit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "yarn test --watchAll=false --coverage",
    "test:storybook": "test-storybook",
    "e2e": "playwright test",
    "report": "playwright show-report",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "gen": "rm -r src/api/generated && orval",
    "tt": "node scripts/tt.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
