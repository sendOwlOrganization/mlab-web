    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "recoil": "^0.7.7"
  },
  "packageManager": "yarn@3.4.1",
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@faker-js/faker": "^7.6.0",
    "@linaria/babel-preset": "^4.4.4",
